{
  "expo": {
    "name": "Quick Measure",
    "slug": "quick-measure-gps",
    "scheme": "quick-measure-gps",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3A7032"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.a-down.quick-measure-gps",
      "buildNumber": "12"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#3A7032"
      },
      "package": "com.adown.quickmeasuregps"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-updates",
        {
          "username": "a-down"
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "$(PRODUCT_NAME) requires access to your location to measure area and distance."
        }
      ],
      "react-native-iap"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "009cf2cd-1186-4e6a-86cd-4fd7cc58e2c7"
      }
    },
    "owner": "a-down",
    "runtimeVersion": {
      "policy": "nativeVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/009cf2cd-1186-4e6a-86cd-4fd7cc58e2c7",
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 5000
    }
  },
  "react-native-google-mobile-ads": {
    "ios_app_id": "ca-app-pub-2810780842614584~1331427803",
    "delay_app_measurement_init": true
  }
}
